<%- partial('_partial/page-full', {item: page}) %>
<!-- If it's the About Page, render the following components -->
<% if (page.type == 'about') { %>
    <%- partial('_partial/about/about-page') %>
<% } %>
<!-- If it's the News Page, render the blog -->
<% if (page.type == 'news') { %>
    <%- partial('_partial/blog') %>
<% } %>
<!-- If it's the Treated Conditions Page, render conditions-page.ejs -->
<% if (page.type == 'conditions') { %>
    <%- partial('_partial/conditions/conditions-page') %>
<% } %>               
<!-- If it's the Services Page, render services-page.ejs -->
<% if (page.type == 'services') { %>
    <%- partial('_partial/services/services-page') %>
<% } %>               
<!-- If it's the TMS Page, render tms-strip.ejs -->
<% if (page.type == 'tms-therapy') { %>
    <%- partial('_partial/services/tms-strip') %>
<% } %> 