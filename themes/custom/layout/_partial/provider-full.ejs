
<!-- Note that on page.ejs, "page" was passed in as the "item" -->
<div class="post-full">
    <img src="<%- item.image %>" alt="<%- item.name %> Portrait">
    <h2 class="post-title"><%- item.name %></h2>
    <p class="post-meta">
        <%- item.credentials %>
    </p>
    <%- item.content %>
    <hr />
</div>